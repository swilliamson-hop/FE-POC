# Generated by scripts/generate-keys.ts
# Private key for Access Certificate (P-256, PEM format)
PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\n...\n-----END PRIVATE KEY-----"

# Certificate chain from sandbox (PEM format, base64 encoded DER)
# Download from sandbox after creating Access Certificate
CERT_CHAIN="-----BEGIN CERTIFICATE-----\n...\n-----END CERTIFICATE-----"

# Public Railway URL (set after first deploy)
SERVICE_URL=https://your-service.up.railway.app

# client_id for OpenID4VP requests (x509_hash: scheme, derived from certificate)
# Run: tsx scripts/generate-keys.ts --print-client-id
CLIENT_ID=x509_hash:sha256:BASE64URL_OF_CERT_THUMBPRINT

# Frontend URL â€“ used as redirect_uri for same-device (mobile) flow
# For local dev: http://localhost:3000
# For mobile testing: your Vercel / public URL
FRONTEND_URL=http://localhost:3000

# CORS allowed origins (comma-separated)
ALLOWED_ORIGINS=http://localhost:3000

# Sandbox trust list base URL
TRUST_LIST_URL=https://bmi.usercontent.opencode.de/eudi-wallet/test-trust-lists

# Port (Railway sets this automatically)
PORT=3001
